    "autoload": {
        "psr-4": {
            "Plugins\\MarketManager\\": "app",
            "Plugins\\MarketManager\\Database\\Factories\\": "database/factories/",
            "Plugins\\MarketManager\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plugins\\MarketManager\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">= 8.0",
        "zhenmu/support": "dev-master",
        "fresns/cmd-word-manager": "dev-master",
        "fresns/market-manager": "dev-master",
        "fresns/plugin-manager": "2.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plugins\\MarketManager\\Providers\\MarketManagerServiceProvider",
                "Plugins\\MarketManager\\Providers\\ExceptionServiceProvider"
            ]
        }
    },
    "repositories": {
        "cmd-word-manager": {
            "type": "vcs",
            "url": "git@gitee.com:fresns/cmd-word-manager.git"
        },
        "plugin-manager": {
            "type": "vcs",
            "url": "git@gitee.com:fresns/plugin-manager.git"
        },
        "market-manager": {
            "type": "vcs",
            "url": "git@gitee.com:fresns/market-manager.git"
        }
    }
}